{
	"name": "n8n-nodes-apprise",
	"version": "0.1.0",
	"description": "n8n community node to work with the Apprise API",
	"license": "MIT",
	"homepage": "https://example.com",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "egvimo"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"preinstall": "npx only-allow pnpm",
		"prepublishOnly": "n8n-node prerelease",
		"reset:database": "rm ~/.n8n-node-cli/.n8n/database.*"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/AppriseApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Apprise/Apprise.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "0.17.0",
		"eslint": "9.39.2",
		"prettier": "3.7.4",
		"release-it": "19.0.6",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"n8n-workflow": "2.3.0"
	}
}
